<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/TipoTicketRegistroList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tipoTicketRegistroHome.wire}"/>

   <param name="tipoTicketRegistroFrom"/>
   <param name="tipoTicketRegistroIdTipoTicketRegistro" value="#{tipoTicketRegistroHome.tipoTicketRegistroIdTipoTicketRegistro}"/>


   <navigation from-action="#{tipoTicketRegistroHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/TipoTicketRegistro.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoTicketRegistroHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/TipoTicketRegistro.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tipoTicketRegistroHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TipoTicketRegistroList.xhtml"/>
      </rule>
   </navigation>

</page>
