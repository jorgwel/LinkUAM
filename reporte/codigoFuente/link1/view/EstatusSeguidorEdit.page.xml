<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/EstatusSeguidorList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{estatusSeguidorHome.wire}"/>

   <param name="estatusSeguidorFrom"/>
   <param name="estatusSeguidorIdEstatusSeguidor" value="#{estatusSeguidorHome.estatusSeguidorIdEstatusSeguidor}"/>


   <navigation from-action="#{estatusSeguidorHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/EstatusSeguidor.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{estatusSeguidorHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/EstatusSeguidor.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{estatusSeguidorHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/EstatusSeguidorList.xhtml"/>
      </rule>
   </navigation>

</page>
