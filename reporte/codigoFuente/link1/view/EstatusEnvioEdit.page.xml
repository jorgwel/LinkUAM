<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/EstatusEnvioList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{estatusEnvioHome.wire}"/>

   <param name="estatusEnvioFrom"/>
   <param name="estatusEnvioIdEstatusEnvio" value="#{estatusEnvioHome.estatusEnvioIdEstatusEnvio}"/>


   <navigation from-action="#{estatusEnvioHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/EstatusEnvio.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{estatusEnvioHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/EstatusEnvio.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{estatusEnvioHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/EstatusEnvioList.xhtml"/>
      </rule>
   </navigation>

</page>
