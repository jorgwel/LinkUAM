<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/ComunidadList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{comunidadHome.wire}"/>

   <param name="comunidadFrom"/>
   <param name="comunidadIdComunidad" value="#{comunidadHome.comunidadIdComunidad}"/>
   <param name="estatusComunidadFrom"/>
   <param name="estatusComunidadIdEstatusComunidad" value="#{estatusComunidadHome.estatusComunidadIdEstatusComunidad}"/>



   <navigation from-action="#{comunidadHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Comunidad.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{comunidadHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Comunidad.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{comunidadHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/ComunidadList.xhtml"/>
      </rule>
   </navigation>

</page>
