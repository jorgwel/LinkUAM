   <param name="tipoEnvioFrom"/>
   <param name="tipoEnvioIdTipoEnvio" value="#{tipoEnvioHome.tipoEnvioIdTipoEnvio}"/>


   <param name="estatusAvisoFrom"/>
   <param name="estatusAvisoIdEstatusAviso" value="#{estatusAvisoHome.estatusAvisoIdEstatusAviso}"/>

   <param name="tipoAvisoFrom"/>
   <param name="tipoAvisoIdTipoAviso" value="#{tipoAvisoHome.tipoAvisoIdTipoAviso}"/>



   <navigation from-action="#{avisoHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/Aviso.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{avisoHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/Aviso.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{avisoHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/AvisoList.xhtml"/>
      </rule>
   </navigation>

</page>
