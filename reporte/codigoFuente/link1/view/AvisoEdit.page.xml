   <param name="envioIdEnvio" value="#{envioHome.envioIdEnvio}"/>
   <param name="comunidadFrom"/>
   <param name="comunidadIdComunidad" value="#{comunidadHome.comunidadIdComunidad}"/>
   <param name="estatusComunidadFrom"/>
   <param name="estatusComunidadIdEstatusComunidad" value="#{estatusComunidadHome.estatusComunidadIdEstatusComunidad}"/>


   <param name="estatusEnvioFrom"/>
   <param name="estatusEnvioIdEstatusEnvio" value="#{estatusEnvioHome.estatusEnvioIdEstatusEnvio}"/>

   <param name="invitacionFrom"/>
   <param name="invitacionIdInvitacion" value="#{invitacionHome.invitacionIdInvitacion}"/>
