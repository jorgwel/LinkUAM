   <param name="preguntaFrom"/>
   <param name="preguntaIdPregunta" value="#{preguntaHome.preguntaIdPregunta}"/>
   <param name="estatusPreguntaFrom"/>
   <param name="estatusPreguntaIdEstatusPregunta" value="#{estatusPreguntaHome.estatusPreguntaIdEstatusPregunta}"/>


   <param name="reporteAbusoFrom"/>
   <param name="reporteAbusoIdReporteAbuso" value="#{reporteAbusoHome.reporteAbusoIdReporteAbuso}"/>
   <param name="accionReporteAbusoFrom"/>
   <param name="accionReporteAbusoIdAccionReporteAbuso" value="#{accionReporteAbusoHome.accionReporteAbusoIdAccionReporteAbuso}"/>

   <param name="estatusReporteAbusoFrom"/>
   <param name="estatusReporteAbusoIdEstatusReporteAbuso" value="#{estatusReporteAbusoHome.estatusReporteAbusoIdEstatusReporteAbuso}"/>

